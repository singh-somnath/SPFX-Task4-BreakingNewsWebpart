{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "breaking-news-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/breakingNews/BreakingNewsWebPart.js",
          "manifest": "./src/webparts/breakingNews/BreakingNewsWebPart.manifest.json"
        }
      ]
    },
    "all-breaking-news-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/allBreakingNews/AllBreakingNewsWebPart.js",
          "manifest": "./src/webparts/allBreakingNews/AllBreakingNewsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BreakingNewsWebPartStrings": "lib/webparts/breakingNews/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "AllBreakingNewsWebPartStrings": "lib/webparts/allBreakingNews/loc/{locale}.js"
  }
}
